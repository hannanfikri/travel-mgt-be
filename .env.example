# Copy to .env and fill in values
# Used by docker-compose.yml

# Public domain for HTTPS (e.g. api.example.com)
DOMAIN=api.example.com

# Email for Let's Encrypt registration/renewals
ACME_EMAIL=you@example.com

# Postgres password for travelmgt_user
POSTGRES_PASSWORD=change_me_to_a_strong_password

# JWT signing key (use a long random string)
JWT_SIGNING_KEY=CHANGE_ME_USE_A_SECURE_KEY
